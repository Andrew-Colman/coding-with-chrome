{
  "editor.tabSize": 2,
  "editor.rulers": [80],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true,
  "javascript.implicitProjectConfig.checkJs": true,
  "cSpell.words": [
    "bootscript",
    "browserconfig",
    "contenthash",
    "msapplication",
    "mstile",
    "offsetblur",
    "option",
    "rightclick",
    "service",
    "sourcemap",
    "splashscreen",
    "worker",
    "xlink",
    "yandex"
  ],
}
